<% @track = TRACKS[request.host] %>
<fieldset class='vote' style='text-align: left;'>
<legend><span class='material-icons' style='vertical-align: middle; color: green;'>query_stats</span>tracks</legend>
<ul>
<li>players: <%= @track.players.keys.length %></li>
<li>waypoints: <%= @track.waypoints.members.length %></li>
<li>tracks: <%= @track.adventures.members.length %></li>
<li>adventures: <%= @track.waypoints.members.length ** @track.waypoints.members.length %></li>
</ul>
</fieldset>